{
    "gitbook": "3.2.2",
    "title": "Cryptlex Docs",
    "plugins": [
        "edit-link",
        "prism",
        "swagger",
        "-highlight",
        "github",
        "anchorjs",
        "favicon"
    ],
    "pluginsConfig": {
        "favicon":{
            "shortcut": "assets/images/favicon.ico",
            "bookmark": "assets/images/favicon.ico"
        },
        "edit-link": {
            "base": "https://github.com/cryptlex/cryptlex-docs/tree/master",
            "label": "Edit This Page"
        },
        "github": {
            "url": "https://github.com/cryptlex/cryptlex-docs"
        },
        "theme-default": {
            "styles": {
                "website": "build/gitbook.css"
            }
        }
    }
}